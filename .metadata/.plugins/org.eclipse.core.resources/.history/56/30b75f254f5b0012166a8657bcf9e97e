package com.vectorone;

import android.widget.FrameLayout;

public class CamOverlay {
	private FrameLayout preview;
	private CamActivity cam;
	
	public CamOverlay(CamActivity cam,FrameLayout preview) {
		this.preview=preview;
		this.cam=cam;
		compasmethod();
	}

	private void compasmethod() {
		preview.setForeground(cam.getResources().getDrawable(R.drawable.roterpunkt));		
	}

}
