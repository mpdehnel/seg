package com.data;

import com.google.android.maps.GeoPoint;

public class Cach {

	private GeoPoint geopoint;
	private String discripton;
	private String name;
	private int teamcolor;

	public Cach(String name,GeoPoint geoPoint,
			String discription) {
		this.name = name;
		this.geopoint= geopoint
		this.discripton = discription;
	}

	public double getDistance() {
		return distance;
	}

	public void setDistance(double distance) {
		this.distance = distance;
	}

	public String getDiscripton() {
		return discripton;
	}

	public void setDiscripton(String discripton) {
		this.discripton = discripton;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public double getDirection() {
		return direction;
	}

	public void setDirection(double direction) {
		this.direction = direction;
	}

	public int getTeamcolor() {
		return teamcolor;
	}

	public void setTeamcolor(int teamcolor) {
		this.teamcolor = teamcolor;
	}
	@Override
	public String toString() {
		
		return this.name+"  "+this.direction+"    "+this.distance;
	}

}
