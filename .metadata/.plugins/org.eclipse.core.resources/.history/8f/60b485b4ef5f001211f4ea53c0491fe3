package com.data;

public class User {
	private String username;
	private String nickname;
	private String team;
	private int totalchaches;
	private int totalpoints;
	private int drawable;

	public User(String username, String nickname, String team,
			int totalchaches, int totalpoints, int drawable) {
		this.drawable = drawable;
		this.nickname = nickname;
		this.username = username;
		this.team = team;
		this.totalchaches = totalchaches;
		this.totalpoints = totalpoints;

	}

	public String getUsername() {
		return username;
	}

	public void setUsername(String username) {
		this.username = username;
	}

	public String getNickname() {
		return nickname;
	}

	public void setNickname(String nickname) {
		this.nickname = nickname;
	}

	public String getTeam() {
		return team;
	}

	public void setTeam(String team) {
		this.team = team;
	}

	public int getTotalchaches() {
		return totalchaches;
	}

	public void setTotalchaches(int totalchaches) {
		this.totalchaches = totalchaches;
	}

	public int getTotalpoints() {
		return totalpoints;
	}

	public void setTotalpoints(int totalpoints) {
		this.totalpoints = totalpoints;
	}

	public int getDrawable() {
		return drawable;
	}

	public void setDrawable(int drawable) {
		this.drawable = drawable;
	}

}
