package com.data;

import java.io.*;

public class Log {
	private FileWriter outFile;
	private PrintWriter out;

	public Log() {
		try {
			outFile = new FileWriter("Text.txt");
		} catch (IOException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		out = new PrintWriter(outFile);
	}

	public void writemessege(String message) {
		out.println(message);
		out.close();

	}

	public static void main(String[] args) {
		Log l = new Log();
		l.writemessege("asd");
	
	}

}
